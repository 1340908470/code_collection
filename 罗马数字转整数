#include <stdio.h>
#include <string.h>

int transform(char x) {
   switch (x) {
      case 'I':{
         return 1;
      }
      case 'V':{
         return 5;
      }
      case 'X':{
         return 10;
      }
      case 'L':{
         return 50;
      }
      case 'C':{
         return 100;
      }
      case 'D':{
         return 500;
      }
      case 'M':{
         return 1000;
      }
      default :{
         return 0;
      }
   }
}

int romanToInt(char * s){ 
   int len = strlen(s);
   int ans = 0;
   int tem = 0;
   for (int i=0;i<len;i++) {
      tem = transform(*(s+i));
      if (tem>=transform(*(s+i+1))) {
         ans += tem;
      } else {
         ans -= tem;
      }
   }
   return ans;
}
