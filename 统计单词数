#include <stdio.h>
#include <string.h>
#include <ctype.h>
void split(char ans[][10],char text[],char aim[]) {
    int m=0,k=0;
    for (int i = 0; i < strlen(text); ++i) {
        if (text[i] != ' ') {
            ans[m][k] = text[i];
            k++;
        } else {
            m++;
            k = 0;
        }
    }
    int tem = 0;
    int count = 0;
    int trans = 0;
    for (int j = 0; j <= m; ++j) {
        if (strcmp(ans[j],aim)==0) {
            if (tem == 0) {
                trans = j;
                count++;
                tem++;
            } else {
                count++;
            }
        }
    }
    if (tem != 0) {
        printf("%d ",count);
        printf("%d",trans);
    } else {
        printf("-1");
    }


}

int main() {
    char aim[10];
    char text[1000000];
    gets(aim);
    for (int i = 0; i < strlen(aim); ++i) {
        aim[i] = tolower(aim[i]);
    }
    gets(text);
    for (int j = 0; j < strlen(text); ++j) {
        text[j] = tolower(text[j]);
    }
    char ans[100000][10];
    split(ans,text,aim);
}
