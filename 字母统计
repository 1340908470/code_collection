#include <stdio.h>
//a97 z122 A65 Z90  32
int main() {
    int arr[123] = {};
    char c;
    while ((c=getchar())!='\n') {
        arr[c]++;
    }
    for (int i = 65; i <= 90; ++i) {
        if (arr[i]!=0) {
            printf("The character %c has presented %d times.\n",i,arr[i]);
        }
    }
    for (int i = 97; i <= 122; ++i) {
        if (arr[i]!=0) {
            printf("The character %c has presented %d times.\n",i,arr[i]);
        }
    }
}
