//1 括号中可空
#include <stdio.h>
#include <string.h>
int main()
{
    char ch[101];
    scanf("%s", &ch);
    int len = strlen(ch);
    int i;
    int count1 = 0, count2 = 0;
    for (i = 0; i < len; i++)
    {
        if(ch[i] == '(') count1 += 1;
        else if(ch[i] == ')') count2 += 1;
  
    char c;
    int i = 0;
    int j = 0;
    while ((c = getchar()) != '\n') {
        if (c=='(') {
            par[i] = c;
            pa[i] = j;
            i++;
        }
        if (c==')') {
            if (par[i-1]=='(' && pa[i-1]<j-1) {
                par[i] = 0;
                par[i-1] = 0;
                i--;
            } else {
                printf("parentheses do not match!");
                return 0;
            }
        }
        j++;
    }
    if (par[0] != 0) {
        printf("parentheses do not match!");
    } else {
        printf("parentheses match!");
    }
}
